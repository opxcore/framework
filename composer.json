{
  "name": "opxcore/framework",
  "description": "The OpxCore Framework.",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.4",
    "opxcore/app": "dev-master",
    "opxcore/container": "^1.0.3",
    "opxcore/container-interface": "^1.0.4",
    "opxcore/config": "^1.0.1",
    "opxcore/config-repository-files": "^1.0.4",
    "opxcore/config-cache-files": "^1.0.4",
    "opxcore/config-environment": "^1.0.6",
    "opxcore/log-manager": "^1.3.1",
    "opxcore/log-file": "^1.0.3",
    "opxcore/profiler": "^1.1.0",
    "opxcore/exception-handler-interface": "^1.0.0",
    "opxcore/exception-handler": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.2"
  },
  "autoload": {
    "files": [
    ],
    "psr-4": {
      "OpxCore\\": "core/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpxCore\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "/storage/opxcore/app/.git"
    },
    {
      "type": "vcs",
      "url": "/storage/opxcore/exception-handler/.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
